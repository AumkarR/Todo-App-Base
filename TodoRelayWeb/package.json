{
  "private": true,
  "scripts": {
    "build": "npm run lint && npm run update-schema && npm run relay-compiler && npm run webpack",
    "lint": "eslint *.js src tools",
    "relay-compiler": "relay-compiler --src src --exclude 'data/**' --schema src/data/schema.graphql",
    "start": "npm run relay-compiler && cross-env BABEL_ENV=es webpack-dev-server --mode development --hot",
    "update-schema": "babel-node tools/updateSchema.js",
    "webpack": "cross-env NODE_ENV=production BABEL_ENV=es webpack --mode production"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "npm run update-schema",
      "git add src/data/schema.*"
    ]
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.8.6",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-relay": "^1.7.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "eslint-config-4catalyzer-react": "^0.4.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "farce": "^0.2.6",
    "found": "^0.3.11",
    "found-relay": "^0.3.0-alpha.13",
    "graphql": "^0.13.2",
    "graphql-relay": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "keycode": "^2.2.0",
    "lint-staged": "^7.1.2",
    "prettier": "^1.12.1",
    "prop-types": "^15.6.1",
    "react": "^16.13.0",
    "react-compiler": "^1.0.3",
    "react-dom": "^16.13.0",
    "react-relay": "^1.7.0",
    "react-runtime": "^1.1.0",
    "relay-compiler": "^1.6.0",
    "relay-local-schema": "^0.7.0",
    "relay-runtime": "^1.6.0",
    "style-loader": "^0.21.0",
    "todomvc-app-css": "^2.1.2",
    "todomvc-common": "^1.0.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3"
  }
}
